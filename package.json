{
	"name": "go-file-undercursor",
	"displayName": "go-file-undercursor",
	"description": "Go to File Under Cursor",
	"publisher": "Seongmock",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"url": "https://github.com/seongmock/go-file-undercursor"
	},
	"activationEvents": [
		"onCommand:extension.go-file-undercursor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.go-file-undercursor",
				"title": "Go File Undercursor(Open File)"
			}
		],
		"keybindings": [
			{
				"command": "extension.go-file-undercursor",
				"key": "ctrl+k ctrl+g",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
